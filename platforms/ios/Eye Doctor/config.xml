<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phrll.eyedoctor" version="1.03" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="AllowInlineMediaPlayback" value="false" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="DisallowOverscroll" value="false" />
    <preference name="EnableViewportScale" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="true" />
    <preference name="MediaPlaybackRequiresUserAction" value="false" />
    <preference name="SuppressesIncrementalRendering" value="false" />
    <preference name="GapBetweenPages" value="0" />
    <preference name="PageLength" value="0" />
    <preference name="PaginationBreakingMode" value="page" />
    <preference name="PaginationMode" value="unpaginated" />
    <feature name="LocalStorage">
        <param name="ios-package" value="CDVLocalStorage" />
    </feature>
    <feature name="Keyboard">
        <param name="ios-package" onload="true" value="IonicKeyboard" />
    </feature>
    <feature name="Console">
        <param name="ios-package" value="CDVLogger" />
    </feature>
    <feature name="Device">
        <param name="ios-package" value="CDVDevice" />
    </feature>
    <feature name="InAppBrowser">
        <param name="ios-package" value="CDVInAppBrowser" />
    </feature>
    <feature name="StatusBar">
        <param name="ios-package" value="CDVStatusBar" />
        <param name="onload" value="true" />
    </feature>
    <preference name="StatusBarOverlaysWebView" value="true" />
    <preference name="StatusBarStyle" value="lightcontent" />
    <feature name="MobileAccessibility">
        <param name="ios-package" onload="true" value="CDVMobileAccessibility" />
    </feature>
    <feature name="AdMob">
        <param name="ios-package" value="CDVAdMobPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Insomnia">
        <param name="ios-package" value="Insomnia" />
    </feature>
    <feature name="Media">
        <param name="ios-package" value="CDVSound" />
    </feature>
    <feature name="NetworkStatus">
        <param name="ios-package" value="CDVConnection" />
    </feature>
    <preference name="AndroidPersistentFileLocation" value="Internal" />
    <feature name="File">
        <param name="ios-package" value="CDVFile" />
        <param name="onload" value="true" />
    </feature>
    <icon height="180" src="res/ios/appicon-60@3x.png" width="180" />
    <icon height="60" src="res/ios/appicon-60.png" width="60" />
    <icon height="120" src="res/ios/appicon-60@2x.png" width="120" />
    <icon height="76" src="res/ios/appicon-76.png" width="76" />
    <icon height="152" src="res/ios/appicon-76@2x.png" width="152" />
    <icon height="40" src="res/ios/appicon-Small-40.png" width="40" />
    <icon height="80" src="res/ios/appicon-Small-40@2x.png" width="80" />
    <icon height="57" src="res/ios/appicon.png" width="57" />
    <icon height="114" src="res/ios/appicon@2x.png" width="114" />
    <icon height="72" src="res/ios/appicon-72.png" width="72" />
    <icon height="144" src="res/ios/appicon-72@2x.png" width="144" />
    <icon height="29" src="res/ios/appicon-small.png" width="29" />
    <icon height="58" src="res/ios/appicon-small@2x.png" width="58" />
    <icon height="50" src="res/ios/appicon-Small-50.png" width="50" />
    <icon height="100" src="res/ios/appicon-Small-50@2x.png" width="100" />
    <name>Eye Doctor</name>
    <description>
        Eye Doctor
    </description>
    <author email="support@phrll.com" href="http://www.phrll.com">
        Phrll Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="orientation" value="portrait" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="StatusBarOverlaysWebView" value="false" />
</widget>
